//FoodOredereVeg


 class FoodOrderVeg   {
	OrderFood obj1 = new OrderFood();
	FoodVarities obj2 = new FoodVarities();
	void Order() {
		Scanner obj = new Scanner(System.in);

		int confirm = obj.nextInt();
		if (confirm == 1) {
			System.out.println("");
			System.out.println("Your Order has been Placed!!!");
			obj1.header();
		} else if (confirm == 2) {
			System.out.println("");
			obj1.header();
		} else {
			System.out.println("");
			System.out.println("****Confirm Type Does not Match****");
			System.out.println("");
			obj2.vegriceCusine();
		}
		obj.close();
	}
	
	void biriyaniType() {
		
		Scanner obj = new Scanner(System.in);
		System.out.println("================================================");
		System.out.println("");
		System.out.print("1.Chicken Briyani");
		System.out.println("\t2.Ambur biryani");
