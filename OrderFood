import java.util.Scanner;

public class OrderFood {
	public static void main(String[] args) {
		header();
	}

	public static void header() {
		Scanner obj = new Scanner(System.in);
		Menu obj1 = new Menu();
		System.out.println("================================================");
		System.out.println("\tWelcome to Online Food Delivery\t".toUpperCase());
		System.out.println("================================================");
		System.out.println("================================================");
//		System.out.println("");
		System.out.print("1.Profile Details");
		System.out.println("\t\t2.Food Items");
		System.out.println("");
		System.out.print("3.FeedBack");
		System.out.println("\t\t\t\t4.LogOut");
//		System.out.println("");
		System.out.println("================================================");
		System.out.println("");
		System.out.print("Enter Your Choice: ");
		try {
		int userdata = obj.nextInt();

		switch (userdata) {

		case 1:
			obj1.logIn();
			break;

		case 2:
			obj1.foodItems();
			break;

		case 3:
			obj1.feedBack();
			break;

		case 4:
			obj1.logOut();
			break;

		default:
			System.out.println("\t-->Invaild choice<--" + "\n\t-->Refresh this Page<--");
		}
		}catch(Exception e) {
			System.out.println("\t***Enter Integer value***");
			header();
		}
		
		obj.close();
	}
}

//Menu

 class Menu extends OrderFood{
	FoodType obj1 = new FoodType();
	OrderFood obj2 = new OrderFood();
	static String userNameData;
	static String userPasswordData;
	static String userAddressData;
	
	public void logIn() {
		System.out.println("================================================");
		System.out.println("\tEnter Your Profile Details\t\t");
		System.out.println("================================================");
		Scanner obj = new Scanner(System.in);
		System.out.println("");
		System.out.print("UserName: ");
		String username = obj.next();
		System.out.println("");
		System.out.print("Password: ");
		String password = obj.next();
		System.out.println("");
		System.out.print("Address(with pincode): ");
		obj.nextLine();
		String address = obj.nextLine();
		System.out.println("");
		userNameData = username;
		userPasswordData = password;
		userAddressData = address;
		System.out.println("-->Check Your Details are Correct<--");
		System.out.println("");
		System.out.println("------------------------------------");
		System.out.println("\tYour UserName: " + userNameData);
		System.out.println("\tYour Password: " + "*********");
		System.out.println("\tYour Address: " + userAddressData);
		System.out.println("------------------------------------");
		System.out.println("");
		System.out.print("1.Yes");
		System.out.println("\t\t2.No");
		System.out.println("");
		System.out.print("Confirm it: ");
		int verify = obj.nextInt();
		System.out.println("");
		if (verify == 1) {
			System.out.println("***Logged In Successfully***");
			System.out.println("");
			obj2.header();
		} else {
			System.out.println("-->ReEnter Your Login ID<--");
			System.out.println("");
			logIn();
		}
		obj.close();
	}

	public void foodItems() {
		Scanner obj = new Scanner(System.in);
		System.out.println("================================================");
		System.out.println("\t\tFood Items Detail\t");
		System.out.println("================================================");
		System.out.println("");
		System.out.print("1.Vegetarian Cusines");

		System.out.println("\t 2.Non-Vegetarian Cusines");
		System.out.println("");
		System.out.print("Enter Your Food Type : ");
		int foodTy = obj.nextInt();
		System.out.println("");
		switch (foodTy) {

		case 1:
			obj1.foodTypes();
			obj1.vegVarities();
			break;

		case 2:
			obj1.foodTypes();
			obj1.nonVegVarities();
			break;

		default:
			System.out.println("");
			System.out.println("\t---->Such Food Type is" + " Not Available in our Shop!!!<----");
			System.out.println("");
			System.out.println("\t---->Choose Available Food Type<----");
			System.out.println("");
			foodItems();
		}
		obj.close();
	}

	public void logOut() {
		System.out.println("");
		System.out.println("\t" + "Visit Again Dear Customer" + "\t");
	}
	
	public static void feedBack() {
		Scanner obj = new Scanner(System.in);
		System.out.println("================================================");
		System.out.println("\t" + "***" + "Username: " + userNameData + "***");
		System.out.println("\t" + "***" + "Address : " + userAddressData + "***");
		System.out.println("================================================");
		System.out.println("");
		System.out.println("Enter Your Feedback:-");
		System.out.print("\t");
		String feed = obj.nextLine();
		System.out.println("");
		System.out.println("***Your Feedback Has Been Accepted***");
		obj.close();
	}
}

//FoodVarieties


 class FoodVarities extends OrderFood{

	public void vegriceCusine() {
		FoodOrderVeg obj2 = new FoodOrderVeg();
		Scanner obj = new Scanner(System.in);
		System.out.println("================================================");
		System.out.println("");
		System.out.print("1.Veg Briyani");
		System.out.println("\t\t2.Fried Rice");
		System.out.println("");
		System.out.print("3.Fried Noodles");
		System.out.println("\t\t4.Egg Rice");
		System.out.println("");
		System.out.println("================================================");
		System.out.println("");
		System.out.print("Enter Your Food Varities: ");
		int foodvarities = obj.nextInt();

		switch (foodvarities) {

		case 1:
			String name = "Veg Briyani";
			System.out.println("================================================");
			System.out.println("-->" + name + "<--");
			int price = 70;
			System.out.println("Cost is: " + "Rs." + price);
			System.out.println("To Place Order -> Enter 1 ");
			System.out.println("To Leave -> Enter 2");
			System.out.println("================================================");
			System.out.println("");
			System.out.print("Enter the Confirmation Type: ");
			obj2.Order();
			break;

		case 2:
			String name1 = "Fried Rice";
			System.out.println("================================================");
