//foodTypes
class FoodType extends OrderFood{
	FoodVarities obj1 = new FoodVarities();

	void foodTypes() {
		System.out.println("================================================");
		System.out.print("1.Fried Cusines");
		System.out.println("\t\t2.Gravy Cusine");
		System.out.println("");
		System.out.print("3.Soup Cusine");
		System.out.println("\t\t4.Meals");
		System.out.println("================================================");

	}

	void vegVarities() {
		Scanner obj = new Scanner(System.in);
		System.out.println("");
		System.out.print("Enter Your Cusine Type: ");
		int cusineData = obj.nextInt();
		System.out.println("");

		switch (cusineData) {

		case 1:
			System.out.println("================================================");
			System.out.println("\t\tFried Cusine");
			System.out.println("================================================");
			obj1.vegriceCusine();
			break;

		case 2:
			System.out.println("================================================");
			System.out.println("\t\tGravy Cusine");
			System.out.println("================================================");
			obj1.vegGravyCusine();
			break;

		case 3:
			System.out.println("================================================");
			System.out.println("\t\tSoup Cusine");
			System.out.println("================================================");
			obj1.vegSoupCusine();
			break;

		case 4:
			System.out.println("================================================");
			System.out.println("\t\tMeals");
			System.out.println("================================================");
			obj1.vegMeals();
			break;
		}
		obj.close();
	}

	void nonVegVarities() {
		Scanner obj = new Scanner(System.in);
		System.out.print("Enter Your Cusine Type: ");
		int cusineData = obj.nextInt();

		switch (cusineData) {

		case 1:
			System.out.println("================================================");
			System.out.println("\t\tFried Cusine");
			System.out.println("================================================");
			obj1.nonVegRiceCusine();
			break;

		case 2:
			System.out.println("================================================");
			System.out.println("\t\tGravy Cusine");
			System.out.println("================================================");
			obj1.nonVegGravyCusine();
			break;

		case 3:
			System.out.println("================================================");
			System.out.println("\t\tSoup Cusine");
			System.out.println("================================================");
			obj1.nonVegSoupCusine();
			break;

		case 4:
			System.out.println("================================================");
			System.out.println("\t\t\tMeals");
			System.out.println("================================================");
			obj1.nonVegMeals();
			break;
		}
		obj.close();

	}
}

